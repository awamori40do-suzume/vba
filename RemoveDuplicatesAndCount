Sub InsertColumnAndMatchWithD()
    Dim ws As Worksheet
    Dim lastRowA As Long
    Dim lastRowD As Long
    Dim i As Long
    Dim uniqueDict As Object
    Dim key As Variant
    Dim rowIndex As Long
    Dim outputIndex As Long
    
    Set ws = ThisWorkbook.Sheets(1) ' 対象のシートを指定
    lastRowA = ws.Cells(ws.Rows.Count, 1).End(xlUp).Row ' A列の最終行を取得
    lastRowD = ws.Cells(ws.Rows.Count, 4).End(xlUp).Row ' D列の最終行を取得
    Set uniqueDict = CreateObject("Scripting.Dictionary") ' Dictionaryオブジェクトを作成
    
    ' B列に列を挿入
    ws.Columns(2).Insert Shift:=xlToRight
    
    ' A列のデータを走査し、重複を削除してカウント
    For i = 1 To lastRowA
        key = ws.Cells(i, 1).Value
        If Not uniqueDict.Exists(key) Then
            uniqueDict.Add key, 1 ' 初めての値を追加
        Else
            uniqueDict(key) = uniqueDict(key) + 1 ' カウントを増加
        End If
    Next i
    
    ' D列と同じ位置にB列にデータを配置
    For i = 1 To lastRowD
        key = ws.Cells(i, 4).Value ' D列のデータ
        If uniqueDict.Exists(key) Then
            ws.Cells(i, 2).Value = key ' D列と同じ位置にB列に転記
            uniqueDict.Remove key ' 転記したデータはDictionaryから削除
        End If
    Next i
    
    ' B列の末尾に、D列に存在しないデータを追加
    rowIndex = lastRowD + 1 ' D列の最後の行の次からスタート
    For Each key In uniqueDict.Keys
        ws.Cells(rowIndex, 2).Value = key ' B列の末尾に追加
        rowIndex = rowIndex + 1
    Next key
    
    ' C列に出現回数を転記
    rowIndex = 1
    For Each key In uniqueDict.Keys
        ws.Cells(rowIndex, 3).Value = uniqueDict(key) ' C列にカウントを転記
        rowIndex = rowIndex + 1
    Next key
End Sub
